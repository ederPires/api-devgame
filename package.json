{
  "name": "api-node",
  "version": "1.0.0",
  "description": "criando api com node e apollo",
  "main": "index.js",
  "scripts": {
    "dev:simple": "tsnd --respawn --transpile-only simple-server.ts",
    "dev:server": "tsnd --respawn --transpile-only src/server.ts",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:rundev": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
    "migration:run": "npm run typeorm migration:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/uuid": "^9.0.8",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "apollo-server": "^3.13.0",
    "graphql": "^16.8.2",
    "graphql-scalars": "^1.23.0",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "type-graphql": "^2.0.0-rc.1",
    "typedi": "^0.10.0",
    "typeorm-typedi-extensions": "^0.4.1",
    "uuid": "^10.0.0",
    "yarn": "^1.22.22"
  }
}
