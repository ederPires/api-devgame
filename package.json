{
  "name": "api-node",
  "version": "1.0.0",
  "description": "criando api com node e apollo",
  "main": "index.js",
  "scripts": {
    "dev": "tsnd --respawn --transpile-only src/server.ts",
    "migration:create": "npx typeorm-ts-node-commonjs migration:create ./src/database/migrations/schema-update -d ./src/database/data-source.ts",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate ./src/database/migrations/schema-update -d ./src/database/data-source.ts",
    "migration:show": "npx typeorm-ts-node-commonjs migration:show -d ./src/database/data-source.ts",
    "migration:run": "npx typeorm-ts-node-commonjs migration:run -d  ./src/database/data-source.ts",
    "migration:revert": "npx typeorm-ts-node-commonjs migration:revert -d ./src/database/data-source.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^9.0.8",
    "ts-node-dev": "^2.0.0",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.2"
  },
  "dependencies": {
    "@prisma/client": "^5.15.1",
    "apollo-server": "^3.13.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "graphql": "^16.8.2",
    "graphql-middleware": "^6.1.35",
    "graphql-scalars": "^1.23.0",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.10.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "sqlite3": "^5.1.7",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "type-graphql": "^2.0.0-rc.1",
    "typedi": "^0.10.0",
    "typeorm-typedi-extensions": "^0.4.1",
    "uuid": "^10.0.0",
    "yarn": "^1.22.22"
  }
}
